{
  "name": "benchmarks-teamstation-dev",
  "version": "0.1.0",
  "private": true,
  "description": "benchmarks.teamstation.dev static export",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/generate-seo-assets.mjs && next build && node scripts/prepare-static-build.mjs",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "verify:static": "bash scripts/verify-static-domain.sh",
    "release:static": "npm run typecheck && npm run lint && npm run build && npm run verify:static",
    "seed:500": "node scripts/seed-benchmarks.js 500 --reset",
    "seed": "node scripts/seed-benchmarks.js",
    "deploy:ftp": "./scripts/deploy-ftp.sh"
  },
  "dependencies": {
    "next": "15.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "typescript": "5.7.3"
  }
}
